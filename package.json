{
  "name": "todo-cli",
  "description": "A dead simple todo app for your terminal.",
  "version": "0.0.0",
  "author": "Ulises Santana @ulisesantana",
  "bin": {
    "todo": "./bin/run"
  },
  "bugs": "https://github.com/ulisesantana/todo-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "lowdb": "^1.0.0",
    "prompts": "^2.3.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/lowdb": "1.0.7",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/prompts": "^2.0.3",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/ulisesantana/todo-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "todo"
  },
  "repository": "ulisesantana/todo-cli",
  "scripts": {
    "lint:fix": "yarn posttest -- --fix",
